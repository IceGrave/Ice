<UserControl x:Class="Whydoisuck.Views.NavigationPanel.NavigationSearchResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Whydoisuck.Views.NavigationPanel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Button Padding="5" Command="{Binding UpdateCommand}" CommandParameter="switch">
        <TextBlock Text="{Binding Group.GroupName}"/>
    </Button>
</UserControl>
