<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:selectedView="clr-namespace:Whydoisuck.Views.SelectedLevel"
                    xmlns:currentView="clr-namespace:Whydoisuck.Views.CurrentLevel"
                    xmlns:VM="clr-namespace:Whydoisuck.ViewModels"
                    xmlns:currentLevelVM="clr-namespace:Whydoisuck.ViewModels.CurrentLevel"
                    xmlns:selectedLevelVM="clr-namespace:Whydoisuck.ViewModels.SelectedLevel"
                    xmlns:settingsVM="clr-namespace:Whydoisuck.ViewModels.AppSettings"
                    xmlns:settingsViews="clr-namespace:Whydoisuck.Views.AppSettings">
    <DataTemplate DataType="{x:Type currentLevelVM:CurrentLevelViewModel}">
        <currentView:CurrentLevelView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type selectedLevelVM:SelectedLevelViewModel}">
        <selectedView:SelectedLevelView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type settingsVM:SettingsViewModel}">
        <settingsViews:Settings/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type VM:DelayedViewModel}">
        <ContentControl Content="{Binding ViewModel}"/>
    </DataTemplate>
</ResourceDictionary>