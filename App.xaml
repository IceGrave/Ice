<Application x:Class="Whydoisuck.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Whydoisuck"
            StartupUri="pack:Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/ResourceDicts/Colors.xaml"/>
                <ResourceDictionary Source="Views/ResourceDicts/NavigatorDataTemplates.xaml"/>
                <ResourceDictionary Source="Views/ResourceDicts/SelectedLevelDataTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
