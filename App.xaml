<Application x:Class="Whydoisuck.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Whydoisuck"
             xmlns:currentLevel="clr-namespace:Whydoisuck.ViewModels.Current"
             xmlns:selectedLevel="clr-namespace:Whydoisuck.ViewModels.SelectedLevel"
             xmlns:currentView="clr-namespace:Whydoisuck.Views.Currentlevel"
             StartupUri="pack:Views/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type currentLevel:CurrentLevelViewModel}">
            <currentView:CurrentLevelView></currentView:CurrentLevelView>
        </DataTemplate>
        <DataTemplate DataType="{x:Type selectedLevel:SelectedLevelViewModel}">
            <TextBlock>PRANK</TextBlock>
        </DataTemplate>
    </Application.Resources>
</Application>
