<Application x:Class="Whydoisuck.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Whydoisuck"
             xmlns:currentLevelVM="clr-namespace:Whydoisuck.ViewModels.Current"
             xmlns:selectedLevelVM="clr-namespace:Whydoisuck.ViewModels.SelectedLevel"
             xmlns:sessionVM="clr-namespace:Whydoisuck.ViewModels.Session"
             xmlns:currentView="clr-namespace:Whydoisuck.Views.Currentlevel"
             xmlns:selectedView="clr-namespace:Whydoisuck.Views.SelectedLevel"
             xmlns:sessionView="clr-namespace:Whydoisuck.Views.SelectedSession"
             StartupUri="pack:Views/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type currentLevelVM:CurrentLevelViewModel}">
            <currentView:CurrentLevelView></currentView:CurrentLevelView>
        </DataTemplate>
        <DataTemplate DataType="{x:Type selectedLevelVM:SelectedLevelViewModel}">
            <selectedView:SelectedLevelView></selectedView:SelectedLevelView>
        </DataTemplate>
        <DataTemplate DataType="{x:Type sessionVM:SessionViewModel}">
            <sessionView:SessionView></sessionView:SessionView>
        </DataTemplate>
    </Application.Resources>
</Application>
